switch(marks)
{
  case 1:
  marks=86;
  printf("For entered marks Grade is A and GP is 4.00");
  break;
  case 2:
  marks=82;
  printf("For entered marks Grade is A- and GP is 3.67");
  break;
  case 3:
  marks=78;
  printf("For entered marks Grade is B+ and GP is 3.33");
  break;
  case 4:
  marks=74;
  printf("For entered marks Grade is B and GP is 3.00");
  break;
  case 5:
  marks=70;
  printf("For entered marks Grade is B- and GP is 2.67");
  break;
  case 6:
